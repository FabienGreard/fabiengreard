extends layout
      
block content
  h1=`Welcome to my blog`
  .content-container
    img.picture(width="75" height="75" src=`/images/picture.jpg` alt=`fabiengreard`)
    .p-container
      p=`Hey, I'm Fabien Gr√©ard a young entrepreneur.`
      p=`I've got an engineer degree, nowadays I try to make cool things.`
      p=`Along with my feelings, posts on this blog talks about daily success and fail...`

block routes  
  h2=`Posts`
  ul.route-link-container
    each link in routes
      if link.name
        li.route-link
          a.link-route(href=`${link.url}` title=`${link.name}`)=link.name

block socials  
  ul.social-container
    each link in socials
      if link.name
        li.social-link
          a.link-social(href=`${link.url}` title=`${link.name}`)
            img.img-social(src=`/images/brands/${link.name}.svg` alt=`${link.name}`)


          

  
