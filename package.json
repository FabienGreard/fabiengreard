{
  "name": "FabienGreard",
  "version": "2.0.0",
  "description": "FabienGreard blog",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FabienGreard/fabiengreard.git"
  },
  "scripts": {
    "start": "node ./config/www",
    "dev": "cross-env NODE_ENV=development nodemon ./config/www",
    "test": "jest  --clearCache && cross-env NODE_ENV=development jest --config ./config/jest.config.js",
    "coverage": "cross-env NODE_ENV=development jest --coverage --watchAll --config ./config/jest.config.js",
    "prettier": "prettier --single-quote --write **/*.{js,json,css,md}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "author": "FabienGreard",
  "license": "MIT",
  "dependencies": {
    "clean-css": "^4.2.1",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "marked": "^0.5.1",
    "morgan": "^1.9.1",
    "pug": "^2.0.3",
    "uglify-js": "^3.4.9",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "supertest": "^3.3.0"
  },
  "keywords": [
    "Server",
    "BasicAuth",
    "Boilerplate",
    "nodejs",
    "javascript",
    "https",
    "http",
    "routes",
    "blog",
    "logs"
  ]
}
