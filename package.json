{
  "name": "fabiengreard",
  "version": "0.0.5",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "dev": "webpack serve --open --config webpack.dev.config.js",
    "lint": "eslint src",
    "prettier": "prettier --write 'src/**/**/*.{js,json,css,md}'",
    "bumb": "standard-version --no-verify && git push --follow-tags"
  },
  "repository": "https://github.com/FabienGreard/fabiengreard.git",
  "author": "fabiengreard <grd.fabien@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "html-webpack-plugin": "^5.1.0",
    "husky": "^4.2.3",
    "prettier": "^2.0.4",
    "prop-types": "^15.7.2",
    "standard-version": "^9.1.0",
    "style-loader": "^1.1.3",
    "styled-components": "^5.0.1",
    "webpack": "^5.21.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-spring": "^8.0.27"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
